{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "./src",
    "paths": {
      "@src/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@commands/*": [
        "./components/commands/*"
      ],
      "@module/*": [
        "./components/module/*"
      ],
      "@timeEvent/*": [
        "./components/timeEvent/*"
      ],
      "@utils/*": [
        "./components/utils/*"
      ],
